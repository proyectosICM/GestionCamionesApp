{
  "expo": {
    "name": "GestionCamiones-app",
    "private": true,
    "dependencies": {
      "expo": "^43.0.0",
      "react": "17.0.2",
      "react-dom": "17.0.2",
      "react-native": "0.64.2",
      "react-native-camera": "^3.59.0",
      "react-native-permissions": "^3.0.0"
    },
    "devDependencies": {
      "jest-expo": "^43.0.0"
    },
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
      "start": "expo start",
      "android": "expo start --android",
      "ios": "expo start --ios",
      "web": "expo start --web",
      "eject": "expo eject"
    },
    "jest": {
      "preset": "jest-expo"
    },
    "expo": {
      "name": "my-app-qr",
      "slug": "my-app-qr",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "permissions": {
        "camera": "required",
        "internet": "required",
        "vibrate": "required"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      }
    }
  }
}
